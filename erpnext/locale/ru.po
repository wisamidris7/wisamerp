msgctxt "Sales Order"
msgid "%  Delivered"
msgstr ""

#. Label of a Percent field in DocType 'Delivery Note'
#: stock/doctype/delivery_note/delivery_note.json
msgctxt "Delivery Note"
msgid "% Amount Billed"
msgstr ""

